{
  "name": "@tombatossals/gatsby-source-spotify",
  "version": "1.3.2",
  "description": "Gatsby source plugin for using data from Spotify on your website",
  "main": "index.js",
  "bin": "token-tool.js",
  "author": "David Rubert <david.rubert@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tombatossals/gatsby-source-spotify.git"
  },
  "homepage": "https://github.com/tombatossals/gatsby-source-spotify",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "spotify"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "tsc",
    "format": "prettier --write 'src/**/*",
    "lint": "tslint --project tsconfig.json"
  },
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/open": "^6.2.1",
    "commander": "^5.1.0",
    "gatsby-node-helpers": "^0.3.0",
    "gatsby-source-filesystem": "^2.2.5",
    "inquirer": "^7.1.0",
    "node-fetch": "^2.6.0",
    "open": "^7.0.3"
  },
  "devDependencies": {
    "@types/inquirer": "^6.5.0",
    "@types/node": "^13.13.4",
    "@types/node-fetch": "^2.5.7",
    "@types/prettier": "^2.0.0",
    "gatsby": "^2.20.36",
    "prettier": "^2.0.5",
    "react-dom": "^16.13.1",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "gatsby": "^2.10.8"
  }
}
